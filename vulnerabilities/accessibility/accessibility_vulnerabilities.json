        {
            "id": "ACC-001",
            "name": "Accessibility Service Abuse",
            "description": "Application's accessibility service can be exploited to perform unauthorized actions or collect sensitive information.",
            "severity": {
                "level": "Critical",
                "impact": "Unauthorized access and data theft",
                "exploitability": "Medium"
            },
            "affects": {
                "component": "android.accessibilityservice.AccessibilityService",
                "methods": [
                    "onAccessibilityEvent",
                    "onServiceConnected",
                    "performGlobalAction"
                ],
                "service": "accessibility_service"
            },
            "detection": {
                "pattern": [
                    "extends\\s+AccessibilityService",
                    "onAccessibilityEvent\\([^)]+\\)",
                    "performGlobalAction\\("
                ],
                "method": "static",
                "indicators": [
                    "Service implementation",
                    "Event handling",
                    "Action performance"
                ],
                "accuracy_improvements": {
                    "required_conditions": [
                        "Service implementation",
